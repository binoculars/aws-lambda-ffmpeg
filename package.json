{
  "name": "aws-lambda-ffmpeg",
  "version": "3.0.0",
  "private": true,
  "description": "AWS Lambda function to resize video and generate a thumbnail",
  "engines-strict": true,
  "engines": {
    "node": ">=4.3",
    "npm": ">=3"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "./scripts/postinstall.js",
    "test": "ava",
    "test-integration": "ava test/integration/**/*.js"
  },
  "keywords": [
    "aws",
    "lambda",
    "ffmpeg",
    "gcp",
    "gcf",
    "cloud function",
    "azure function"
  ],
  "author": "Barrett Harber",
  "license": "Unlicense",
  "devDependencies": {
    "ava": "^0.18.2",
    "aws-sdk-on-lambda": "*",
    "azure-storage": "^2.1.0",
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "chalk": "^1.1.3",
    "del": "^2.2.0",
    "google-cloud": "^0.49.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-chmod": "^2.0.0",
    "gulp-install": "^0.6.0",
    "gulp-json-editor": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^4.0.0",
    "run-sequence": "^1.0.2",
    "simple-git": "^1.67.0"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "exclude": [
            "transform-es2015-arrow-functions",
            "transform-es2015-for-of",
            "check-es2015-constants",
            "transform-regenerator"
          ]
        }
      ]
    ]
  },
  "ava": {
    "files": "test/unit/**/*.js",
    "require": [
      "babel-register"
    ]
  }
}
